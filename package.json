{
  "name": "copy-filename-content",
  "displayName": "Copy Filename and Content",
  "description": "Copy file name and content to clipboard",
  "version": "0.1.7",
  "publisher": "wangqin",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangqin0/copy-filename-content"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copy-filename-content.copy",
        "title": "Copy Filename and Content"
      },
      {
        "command": "copy-filename-content.copyAllWorkspaces",
        "title": "Copy All Workspace Content"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "copy-filename-content.copy",
          "group": "clipboard"
        }
      ],
      "commandPalette": [
        {
          "command": "copy-filename-content.copyAllWorkspaces"
        }
      ]
    },
    "configuration": {
      "title": "Copy Filename Content",
      "properties": {
        "copy-filename-content.excludePatterns": {
          "type": "array",
          "default": [],
          "description": "Additional patterns to exclude when copying"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn compile",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "all": "yarn install && yarn compile"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "typescript": "^4.5.5"
  }
}